<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="@color/white"
	>
	<RelativeLayout

		android:layout_width="match_parent"
		android:layout_height="40dp"




		android:background="@drawable/number_base"

		>

		<LinearLayout
			android:orientation="vertical"
			android:id="@+id/ascv_sample_holder"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="125dp"
			>

		</LinearLayout>

	</RelativeLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<ImageView
			android:id="@+id/vp_shipin"
			android:background="@drawable/sucai"
			android:layout_width="match_parent"
			android:layout_height="158dp">

		</ImageView>
		<TextView
			android:layout_width="match_parent"
			android:layout_height="20dp"
			android:text="大师之路    接受特训                                                                滑动查看更多"
			android:textSize="12dp"
			android:gravity="center_vertical"
			android:textColor="@color/black"
			android:background="@drawable/number_base"/>
		<LinearLayout

			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:orientation="horizontal"
			android:background="@color/base_tab_indicator_text_color">
          <include layout="@layout/dashi"></include>

		</LinearLayout>
		<TextView
			android:layout_width="match_parent"
			android:layout_height="10dp"
			android:background="@drawable/number_base"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="400dp">
			<include layout="@layout/texun"></include>

		</LinearLayout>


	</LinearLayout>



</LinearLayout>

